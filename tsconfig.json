{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
		"noEmit": true,
		"allowJs": false,
		"checkJs": false,
		"jsx": "react-jsx",
		"skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["vite/client"]
	},
    "include": ["src"]
}
